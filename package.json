{
  "name": "@digidem/extract-react-intl-messages",
  "version": "1.0.2",
  "description": "Extract react-intl messages",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/extract-react-intl-messages.git"
  },
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{json,js,md}'",
    "example": "./cli.js -l=en,ja -o example/i18n -d en 'example/**/*.js'",
    "test": "xo && ava"
  },
  "bin": {
    "extract-react-intl-messages": "cli.js",
    "extract-messages": "cli.js"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "react",
    "i18n",
    "intl",
    "react-intl",
    "extract",
    "json",
    "messages"
  ],
  "dependencies": {
    "@digidem/extract-react-intl": "^0.10.0",
    "flat": "^4.1.0",
    "lodash": "^4.17.15",
    "meow": "^5.0.0",
    "mkdirp": "^0.5.1",
    "sort-keys": "^3.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.7.0",
    "ava": "^2.2.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-intl": "^2.9.0",
    "temp-write": "^4.0.0",
    "tempy": "^0.3.0",
    "xo": "^0.24.0"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "operator-linebreak": 0,
      "indent": 0
    }
  },
  "ava": {
    "helpers": [
      "**/fixtures/**/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bugs": {
    "url": "https://github.com/digidem/extract-react-intl-messages/issues"
  },
  "homepage": "https://github.com/digidem/extract-react-intl-messages#readme",
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  }
}
